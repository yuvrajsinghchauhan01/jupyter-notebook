   "source": [
    "def fibonacii_num(n):\n",
    "    #base condition\n",
    "    if n<0:\n",
    "        return \"not defined\"\n",
    "    elif n==0:\n",
    "        return 0\n",
    "    elif n==1 or n==2:\n",
    "        return 1\n",
    "    else:\n",
    "    #recursive formula\n",
    "        return fibonacii_num(n-1)+fibonacii_num(n-2)"
   ]
